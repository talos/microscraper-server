
{
    "description" : "A MicroScraper scrapeable web page",
    "type" : "object",
    "properties" : {
	"url" : {
	    "description" : "The requested URL, which is mustached.",
	    "type" : "string",
	    "format" : "uri",
	    "required" : true
	},
	"method" : {
	    "description": "The HTTP method to use.",
	    "type" : "string",
	    "enum" : ["head", "get", "post"],
	    "default" : "get"
	},
	"posts" : {
	    "type" : "object"
	},
	"cookies" : {
	    "type" : "object"
	},
	"headers" : {
	    "type" : "object"
	},
	"loadBefore" : {
	    "type" : "array",
	    "items" : {
		"type" : { "$ref" : "{$ref}" }
	    }
	}
    }
}
