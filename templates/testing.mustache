<div class="test ui-widget-content">
  <div class="code">
    JSON (<a href="{{location}}?format=json">link</a>):<br>
    <code class="json">
      {{to_json}}
    </code>
  </div>
  {{> applet}}
  <ul class="variables">
    <form class="test" method="get" action="{{location}}?format=json">
      {{#variables}}
        <li class="variable">
          <label for="{{to_s}}">{{to_s}}:</label>
          <textarea name="{{to_s}}"></textarea>
        </li>
      {{/variables}}
      <button type="submit">Test</button>
    </form>
  </ul>
</div>

